{
  "extends": [
    "github>rancher/renovate-config#release"
  ],
  "baseBranches": [
    "main",
    "release/v0.3"
    "release/v0.4"
  ],
  "prHourlyLimit": 2,
  "packageRules": [
    {
      "enabled": false,
      "matchPackageNames": [
        "/k8s.io/*/",
        "/sigs.k8s.io/*/",
        "/github.com/prometheus/*/"
      ]
    }
  ]
}
